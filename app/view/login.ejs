<link rel="stylesheet" type="text/css" href="/public/css/login.css">
<div>欢迎登陆后台管理系统</div>

<script>
    let cookie = function () {
        let c = document.cookie;

        if (!c) {
            return c;
        } else {
            return [{}, ...c.split(';')].reduce((target, item) => {
                let [key, value] = item.split('=');
                target[key.trim()] = decodeURIComponent(value);
                return target;
            });
        }
    }();

    fetch('http://localhost:3001/api/user/logout', {
        method: 'POST',
        body: JSON.stringify({ a: 232424 })
    }).then(async res => {
        const response = await res.json();
        console.log(response);
    });
</script>
